<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.skripsi.apmodasi">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Apmodasi"
        tools:targetApi="m">
        <activity android:name=".ui.activity.kader.DetailBayiKaderActivity"></activity>
        <activity android:name=".ui.activity.kader.DetailBundaActivity" />
        <activity android:name=".ui.activity.kader.InputDataBayiActivity" />
        <activity android:name=".ui.activity.kader.ScanActivity" />
        <activity android:name=".ui.activity.kader.HistoryKaderActivity" />
        <activity android:name=".ui.activity.kader.AkunKaderActivity" />
        <activity android:name=".ui.activity.kader.DataIbuActivity" />
        <activity android:name=".ui.activity.kader.DataBayiActivity" />
        <activity android:name=".ui.activity.kader.MenuKaderActivity" />
        <activity android:name=".ui.activity.bunda.EditBayiActivity" />
        <activity android:name=".ui.activity.bunda.DetailBayiActivity" />
        <activity android:name=".ui.activity.intro.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.bunda.AkunActivity" />
        <activity android:name=".ui.activity.ImageViewActivity" />
        <activity android:name=".ui.activity.bunda.MenuActivity" />
        <activity android:name=".ui.activity.intro.LoginActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>