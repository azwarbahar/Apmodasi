<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.skripsi.apmodasi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Apmodasi">
        <activity android:name=".ui.activity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.AkunActivity" />
        <activity android:name=".ui.activity.ImageViewActivity" />
        <activity android:name=".ui.activity.MenuActivity">
        </activity>
        <activity android:name=".ui.activity.LoginActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>